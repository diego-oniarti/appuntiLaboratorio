#include <iostream>
using namespace std;

int main(){
  float a,b,c;

  cout << "Inserire A, B e C:"<<endl;
  cin >> a >> b >> c;

  if (a>b) goto escludiA;
  //B Ã¨ escluso
  if (a>c) goto C;
  goto A;
 escludiA:
  if (b>c) goto C;
  goto B;
  
 A:
  cout << a;
  goto fine;
 B:
  cout << b;
  goto fine;
 C:
  cout << c;
 fine:
  cout << endl;
  return 0;
}
